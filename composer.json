{
  "name": "datatypes/datatypes",
  "description": "Data type classes for PHP",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "data",
    "types"
  ],
  "authors": [
    {
      "name": "Michael Hall",
      "homepage": "https://michaelhall.se/"
    }
  ],
  "require": {
    "php": ">=7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "DataTypes\\": "src"
    },
    "files": [
      "deprecated.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "DataTypes\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "2.4-dev"
    }
  }
}
